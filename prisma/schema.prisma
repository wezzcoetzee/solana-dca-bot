generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Transaction {
  id       String   @id @default(uuid())
  date     DateTime @default(now())
  wallet   String   @db.VarChar(64)
  amount   Float
  btcPrice Float    @map("btc_price")

  @@index([wallet])
  @@map("transactions")
}
